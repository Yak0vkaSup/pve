    "jsonwebtoken": "^9.0.2",
    "lightweight-charts": "^4.2.0",
    "litegraph.js": "^0.7.18",
    "pinia": "^2.2.4",
    "socket.io-client": "^4.7.5",
    "vue": "^3.4.29",
    "vue-chartjs": "^5.3.2",
    "vue-router": "^4.3.3",
    "vue3-toastify": "^0.2.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.8.0",
